@inproceedings{weinkauf05a,
 abstract             = {This paper presents an approach to extracting and classifying higher order critical points of 3D vector fields. To do so, we place a closed convex surface s around the area of interest. Then we show that the complete 3D classification of a critical point into areas of similar flow behavior is equivalent to extracting the topological skeleton of an appropriate 2D vector field on s, if each critical point is equipped with an additional Bit of information. Out of this skeleton, we create an icon which replaces the complete topological structure inside s for the visualization. We apply our method to find a simplified visual representation of clusters of critical points, leading to expressive visualizations of topologically complex 3D vector fields.},
 address              = {Minneapolis, U.S.A.},
 author               = {T.~Weinkauf and H.~Theisel and K.~Shi and H.-C.~Hege and H.-P.~Seidel},
 booktitle            = {Proc. IEEE Visualization 2005},
 day                  = {23 - 28},
 month                = {October},
 pages                = {559--566},
 title                = {Extracting Higher Order Critical Points and Topological Simplification of {3D} Vector Fields},
 url                  = {http://tinoweinkauf.net/publications/absweinkauf05a.html},
 year                 = {2005},
 }

