 doi                  = {10.2312/conf/EG2012/stars/075-094},
 location             = {Cagliari, Italy},
 pages                = {75--94},
 title                = {Illustrative Flow Visualization: State of the Art, Trends and Challenges},
 url                  = {http://diglib.eg.org/EG/DL/conf/EG2012/stars/075-094.pdf},
